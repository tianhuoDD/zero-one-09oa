<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zeroone.star.filemanager.mapper.FileAttachmentSharelistMapper">

    <insert id="insertDownSharelist">
        insert into file_attachment_sharelist(attachment_xid, xshareList, xorderColumn)
        values (#{attachmentXid},#{xshareList},#{xorderColumn})
    </insert>
    <insert id="insertEditSharelist">
        insert into file_attachment_editorlist(attachment_xid, xeditorList,xorderColumn)
        values (#{attachmentXid},#{xeditorList},#{xorderColumn})
    </insert>
</mapper>

		"echarts": "^5.5.0",
		"element-plus": "^2.8.6",
		"js-pinyin": "^0.2.7",
		"jsoneditor": "^10.1.0",
		"lodash": "^4.17.21",
		"lodash-es": "^4.17.21",
		"oa-frontend": "file:",
		"pdfobject": "^2.3.0",
		"pinia": "^2.1.7",
		"pinia-plugin-persistedstate": "^4.1.2",
